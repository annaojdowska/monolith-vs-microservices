{
    "Logging": {
      "LogLevel": {
        "Default": "Warning",
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
  },
  "AllowedHosts": "*",
  "spring" : {
    "application" : {
      "name" : "cityservice"
    }
  },
  "eureka" : {
    "client" : {
      "shouldRegisterWithEureka" : true,
      "serviceUrl" : "http://eureka:8080/eureka/v2",
      "ValidateCertificates":  false
    },
    "instance" : {
      "appName" : "cityservice",
      "hostName" : "cityservice",
      "port" : "3001"
    }
  }
}
